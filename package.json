{
  "name": "n8n-nodes-rubika",
  "version": "1.0.0",
  "description": "n8n community node for Rubika Bot API",
  "keywords": [
    "n8n-community-node-package",
    "rubika",
    "bot",
    "messaging",
    "iran"
  ],
  "license": "MIT",
  "homepage": "https://github.com/ghahremany/n8n-nodes-rubika",
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ghahremany/n8n-nodes-rubika.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -s"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/RubikaBotApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/RubikaBot/RubikaBot.node.js",
      "dist/nodes/RubikaWebhook/RubikaWebhook.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "~5.45.0",
    "eslint-plugin-n8n-nodes-base": "~1.11.0",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "typescript": "~4.8.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
